<%= form_for(@userrail, html: { class: "form-horizontal", role: "form" }) do |f| %>
  <% if @userrail.errors.any? %>
    <div class="alert alert-danger alert-dismissable" role="alert">
      <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
      <h4><%= pluralize(@userrail.errors.count, "error") %> prohibited this userrail from being saved:</h4>

      <ul>
      <% @userrail.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :g, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :g, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :scaffold, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :scaffold, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :user, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :user, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :first_name, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :first_name, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :last_name, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
      <%= f.text_field :last_name, class: "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label :display_name, class: "col-sm-2 control-label" %>
    <div class="col-sm-10">
